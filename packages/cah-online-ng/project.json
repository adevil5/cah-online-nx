{
  "name": "cah-online-ng",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "cah",
  "sourceRoot": "packages/cah-online-ng/src",
  "tags": [
    "type:app"
  ],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/cah-online-ng",
        "index": "packages/cah-online-ng/src/index.html",
        "main": "packages/cah-online-ng/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "packages/cah-online-ng/tsconfig.app.json",
        "assets": [
          "packages/cah-online-ng/src/favicon.ico",
          "packages/cah-online-ng/src/assets"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/purple-green.css",
          "packages/cah-online-ng/src/styles.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "packages/cah-online-ng/src/environments/environment.ts",
              "with": "packages/cah-online-ng/src/environments/environment.production.ts"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "packages/cah-online-ng/src/environments/environment.ts",
              "with": "packages/cah-online-ng/src/environments/environment.development.ts"
            }
          ]
        },
        "local": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "local"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "cah-online-ng:build:production"
        },
        "development": {
          "browserTarget": "cah-online-ng:build:development"
        },
        "local": {
          "browserTarget": "cah-online-ng:build:local"
        }
      },
      "defaultConfiguration": "local"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "cah-online-ng:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/cah-online-ng/**/*.ts",
          "packages/cah-online-ng/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/cah-online-ng/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "cah-online-ng:build"
      }
    },
    "generate-supabase-types": {
      "command": "node --unhandled-rejections=strict -r ts-node/register packages/cah-online-ng/generate-supabase-types.ts"
    }
  }
}